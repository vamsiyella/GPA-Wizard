{
    "name": "FBLA project",
    "version": "1.0.0",
    "description": "GPA calculator",        
    "manifest_version": 3,
    "author": "Vamsi Yella",
    "action": {
        "default_popup": "index.html",
        "default_title": "GPA"
    },
